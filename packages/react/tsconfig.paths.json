        "./utils"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@hooks": [
        "./hooks"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@components": [
        "./components"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@widgets": [
        "./widgets"
      ],
      "@widgets/*": [
        "./widgets/*"
      ],
      "@types": [
        "@storysdk/types"
      ],
      "@constants": [
        "./constants"
      ],
      "@assets": [
        "./assets"
      ],
      "@assets/*": [
        "./assets/*"
      ]
    }
  },
}